# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'ArcanaDatabase' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'AlamofireImage',  '~> 3.0'
    pod 'Kanna', :git => 'https://github.com/tid-kijyun/Kanna', :branch => 'swift3.0'
    pod 'Firebase'
    pod 'Firebase/Database'
    pod 'Firebase/Storage'
    pod 'Firebase/Auth'
    pod 'SwiftyJSON'
    
  # Pods for ArcanaDatabase
  

  target 'ArcanaDatabaseTests' do
     inherit! :search_paths
  pod 'Kanna', :git => 'https://github.com/tid-kijyun/Kanna', :branch => 'swift3.0'
    pod 'Firebase'
    pod 'Firebase/Database'
    pod 'Firebase/Storage'
    pod 'Firebase/Auth'
    pod 'SwiftyJSON'
end

end
